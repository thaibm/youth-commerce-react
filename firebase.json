{
  "hosting": {
    "public": "public",
    "ignore": ["firebase.json", "**/.*", "**/node_modules/**"],
    "rewrites": [
      {
        "source": "product-details/**",
        "destination": "/product-details.html"
      },
      {
        "source": "**",
        "destination": "/index.html"
      }
    ]
  }
}
